\begin_layout Plain Layout

  @statistic[receptionPower](source="receptionMinSignalPower(radioMedium.signalAr
rivalStarted)";
\end_layout

\begin_layout Plain Layout

record=vector,stats; interpolationmode=linear);
\end_layout

\begin_layout Plain Layout

        @statistic[coordX](title="coord X";
\end_layout

\begin_layout Plain Layout

source="xCoord(mobilityPos(mobilityStateChanged))";
\end_layout

\begin_layout Plain Layout

record=vector?; interpolationmode=linear);
\end_layout

\begin_layout Plain Layout

        @statistic[coordY](title="coord Y";
\end_layout

\begin_layout Plain Layout

source="yCoord(mobilityPos(mobilityStateChanged))";
\end_layout

\begin_layout Plain Layout

record=vector?; interpolationmode=linear);
\end_layout

