\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

x_arr = [];
\end_layout

\begin_layout Plain Layout

y_arr = [];
\end_layout

\begin_layout Plain Layout

z_arr = [];
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

powers = open('power.csv', 'r');
\end_layout

\begin_layout Plain Layout

coors = open('Ypos.csv','r');
\end_layout

\begin_layout Plain Layout

mini = 1;
\end_layout

\begin_layout Plain Layout

cnt = 0;
\end_layout

\begin_layout Plain Layout

powers.readline()
\end_layout

\begin_layout Plain Layout

coors.readline()
\end_layout

\begin_layout Plain Layout

for line in powers:
\end_layout

\begin_layout Plain Layout

    arr = line.split(",");
\end_layout

\begin_layout Plain Layout

    arr = [x.strip() for x in arr]
\end_layout

\begin_layout Plain Layout

    power = [float(arr[0]), 10*math.log10(float(arr[1]))]
\end_layout

\begin_layout Plain Layout

    for line_pos in coors:
\end_layout

\begin_layout Plain Layout

        pos = line_pos.split(",");
\end_layout

\begin_layout Plain Layout

        pos = [float(x.strip()) for x in pos]
\end_layout

\begin_layout Plain Layout

        if pos[1] == 750 and pos[2] == 300:
\end_layout

\begin_layout Plain Layout

            continue;
\end_layout

\begin_layout Plain Layout

        temp = abs(power[0] - pos[0]);
\end_layout

\begin_layout Plain Layout

        if power[0] - pos[0] < 3.0e-03:
\end_layout

\begin_layout Plain Layout

            x_arr.append(pos[1])
\end_layout

\begin_layout Plain Layout

            y_arr.append(pos[2])
\end_layout

\begin_layout Plain Layout

            z_arr.append(power[1])
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

            #print [power[0], power[1], pos[1], pos[2]]
\end_layout

\begin_layout Plain Layout

            break;
\end_layout

\begin_layout Plain Layout

print mini
\end_layout

\begin_layout Plain Layout

print cnt
\end_layout

\begin_layout Plain Layout

print 2*math.pow(10,-10)
\end_layout

\begin_layout Plain Layout

ax.plot(x_arr, y_arr, z_arr, label='parametric curve',facecolors=img)
\end_layout

